import java.util.ArrayList;

public class wordWriter {
	
	private String word; 
	private ArrayList<String> total; 
	private String[] letters; 
	
	public wordWriter() {
		word = ""; 
		total = new ArrayList<>(); 
		letters = {"a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o", "p", "q", "r", "s", "t", "u", "v", "w", "x", "y", "z"}; 
	}
	
	
	
	private void addWords() {
		for(int i = 0; i < 26; i++) {
			for(int j = 0; j < 26; j++) {
				for(int k = 0; k < 26; k++) {
					for(int l = 0; l < 26; l++) {
						for(int m = 0; m < 26; m++) {
							
						}
					}
				} 
			}
		}
		
		
		
	}
	

}
